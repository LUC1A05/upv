package org.pmobo.packlaborategia6;

public class Langilea 
{
	private int id;
	private String izena;
	private String abizena;
	private KontzeptuZerrenda zerrenda;
	
	public Langilea(int pId, String pIzena, String pAbizena, KontzeptuZerrenda pZerrenda)
	{
		this.id = pId;
		this.izena = pIzena;
		this.abizena = pAbizena;
		this.zerrenda = pZerrenda;
	}
	
	public double getSoldata()
	{
		return this.zerrenda.getSoldata();
	}
}
